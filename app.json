{
  "expo": {
    "name": "minaret",
    "slug": "minaret",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/appicon3.png",
    "userInterfaceStyle": "dark",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/appicon3.png",
      "resizeMode": "contain",
      "backgroundColor": "#111"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/appicon3.png",
        "backgroundColor": "#111"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false
    },
    "web": {
      "favicon": "./assets/appicon3.png"
    },
    "plugins": ["expo-font"]
  }
}
